{
  "name": "ai-navigation-api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test-model": "tsx scripts/test-model.ts",
    "extract-keywords": "tsx scripts/extract-keywords.ts",
    "generate-synthetic": "tsx scripts/generate-synthetic-data.ts",
    "generate-100x": "tsx scripts/generate-100x-synthetic-data.ts",
    "export-queries": "tsx scripts/export-query-data.ts",
    "view-queries": "tsx scripts/view-queries.ts",
    "scrape-queries": "tsx scripts/scrape-and-generate-queries.ts"
  },
  "dependencies": {
    "@carlwahlen/navigation-core": "workspace:*",
    "@carlwahlen/shared-types": "workspace:*",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.7.0",
    "typescript": "^5.2.2",
    "tsc-alias": "^1.8.8"
  }
}

