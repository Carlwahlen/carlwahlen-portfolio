{
  "name": "carlwahlen-monorepo",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:portfolio": "vite",
    "dev:ai-api": "pnpm --filter ai-navigation-api dev",
    "dev:all": "pnpm --parallel --filter \"./apps/*\" dev",
    "build": "vite build",
    "build:portfolio": "vite build",
    "build:ai-api": "pnpm --filter ai-navigation-api build",
    "lint": "pnpm --recursive lint",
    "type-check": "pnpm --recursive type-check",
    "generate-sitemap": "tsx scripts/generate-sitemap.ts"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "react-helmet-async": "^2.0.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "typescript": "^5.2.2",
    "tsx": "^4.7.0",
    "vite": "^5.0.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
